<div class="container">
  <div class="row">
    <div class="col-6">
      <%= simple_form_for [@experience] do |f| %>
        <%= f.input :name %>
        <%= f.input :city %>
        <%= f.input :price %>
        <%= f.input :description %>
        <%= f.button :submit, class: 'btn btn-primary mt-3'%>
      <% end %>
    </div>
  </div>
</div>
